/**
 * Definition for a binary tree node.
 * struct TreeNode {
 *     int val;
 *     struct TreeNode *left;
 *     struct TreeNode *right;
 * };
 */
int maxDepth(struct TreeNode* root) {
    if(root==NULL) return 0;
    int l=maxDepth(root->left)+1;
    int r=maxDepth(root->right)+1;
    return l>r?l:r;
}

/*
  求二叉树的最大深度，直接递归啦。。。
*/
