class Solution {
public:
    bool isAnagram(string s, string t) {
   // int len1=strlen(s),len2=strlen(t);
    if(s.length()!=t.length()) return false;
    sort(s.begin(),s.end());
    sort(t.begin(),t.end());
    for(int i=0;i<s.length();i++){
        if(s[i]!=t[i]) return false;
    }
    return true;
    }
};
/*
  判断两个字符串是否是 第一个经过重排得到的。
  直接排序判断啦。。。
  
  当然也想到了用hashtable，懒得写了。。。
*/
